{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/play-spot-services/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/play-spot-services/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/play-spot-services",
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "play-spot-services": {
      "type": "application",
      "root": "apps/play-spot-services",
      "entryFile": "main",
      "sourceRoot": "apps/play-spot-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/play-spot-services/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json",
        "assets": [
          "**/*.proto"
        ],
        "watchAssets": true
      }
    }
  }
}